<xsd:schema version="1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            elementFormDefault="qualified">
    <xsd:element name="material" type="Material" />
    <xsd:complexType name="Material">
        <xsd:sequence>
            <xsd:element name="matlId" type="xsd:int"/>
            <xsd:element name="matlName" type="xsd:string"/>    
            <xsd:element name="matlRank" type="xsd:int"/>
            <xsd:element name="originRef" type="xsd:string"/>  
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>

<xsd:schema version="1.0"
           xmlns:xsd="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified">
    <xsd:element name="products" type="Product" />
    <xsd:complexType name="Product">
        <xsd:sequence>
            <xsd:element name="product_id" type="xsd:string"/>
            <xsd:element name="product_name" type="xsd:string"/>
            <xsd:element name="p_room" type="xsd:int"/>
            <xsd:element name="p_type" type="xsd:int"/>
            <xsd:element name="p_height" type="xsd:int"/>
            <xsd:element name="p_width" type="xsd:int"/>
            <xsd:element name="p_length" type="xsd:int"/>
            <xsd:element name="p_material" type="xsd:int"/>
            <xsd:element name="price" type="xsd:double"/>
            <!--<xsd:element name="image" type="xsd:string"/>-->                      
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
